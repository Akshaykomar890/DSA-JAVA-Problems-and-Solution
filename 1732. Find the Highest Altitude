1732. Find the Highest Altitude

class Solution {
    public int largestAltitude(int[] gain) {
       int high =0;
       int current=0;

     for(int i=0; i<=gain.length-1; i++){
         current = current+gain[i];
         if(current>high){
             high=current;
         }
     }
     return high;
    }
  }
